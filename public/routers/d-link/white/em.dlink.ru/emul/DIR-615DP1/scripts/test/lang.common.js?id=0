var LANG=new function(){var $$={lang:null,template:"scripts/%s.lng.js"};this.template=function(template){$$.template=template},this.lang=function(){return $$.lang},this.list=function(){return window.langs},this.change=function(lang,save){$$.lang=lang,jQuery.getScript($$.template.replace("%s",$$.lang),function(){$(window).trigger("langload",lang),translate()}),(is.unset(save)||save)&&device.system.setLang($$.lang,function(data){data&&20==data.status&&MENU.open(MENU.path())}),$(window).trigger("langchange",lang)},this.update=function(data){var lang=window.curlang;data&&20==data.status&&data.lang&&(lang=data.lang),lang&&this.change(lang,!1)},this.load=function(){deleteCookie("cookie_lang"),device.system.getLang(callback(this,this.update))}};